{
    "name": "atwx/silvergate-client",
    "description": "A Silverstripe module to allow you to log into any of your Silverstripe sites from one place.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "module",
        "management",
        "sites",
        "login",
        "silvergate",
        "atwx"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "ext-openssl": "*",
        "silverstripe/framework": "^6.0",
        "silverstripe/admin": "^3.0",
        "firebase/php-jwt": "^6.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "squizlabs/php_codesniffer": "^3.7",
        "silverstripe/standards": "^1",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Atwx\\SilverGateClient\\": "src/",
            "Atwx\\SilverGateClient\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
